{
  "name": "rent-manager-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev & wait-on http://localhost:3000 && npx react-scan@latest http://localhost:3000/",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check . --fix",
    "format": "biome format . --write",
    "format:check": "biome format .",
    "check": "biome check . && biome format . --check",
    "check:fix": "biome check . --fix && biome format . --write",
    "type-check": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "check-all": "npm run lint && npm run format:check && npm run type-check",
    "fix-all": "npm run lint:fix && npm run format",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:ci": "cypress run --headless --browser chrome",
    "prepare": "husky",
    "scan": "npx react-scan@latest http://localhost:3000/"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "add": "^2.0.6",
    "button": "^1.1.1",
    "card": "^2.5.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "input": "^1.0.1",
    "label": "^0.2.2",
    "lucide-react": "^0.536.0",
    "next": "15.4.5",
    "npx": "^10.2.2",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "shadcn": "^2.10.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cypress": "^14.5.4",
    "cypress-plugin-tab": "^1.0.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome check --fix",
      "biome format --write"
    ],
    "*.{json,md,css}": [
      "biome format --write"
    ]
  }
}
